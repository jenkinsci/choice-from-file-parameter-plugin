<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.jenkins-ci.plugins</groupId>
        <artifactId>plugin</artifactId>
        <version>1.409</version>
    </parent>
    <artifactId>choice-from-file-parameter</artifactId>
    <packaging>hpi</packaging>
    <name>Choice From File Parameter Plugin</name>
    <version>1.0</version>
    <url>http://wiki.jenkins-ci.org/display/JENKINS/Choice+From+File+Parameter+Plugin</url>

    <scm>
        <connection>scm:git:git://github.com/jenkinsci/choice-from-file-parameter-plugin.git</connection>
        <developerConnection>scm:git:git@github.com:jenkinsci/choice-from-file-parameter-plugin.git</developerConnection>
        <url>http://github.com/jenkinsci/choice-from-file-parameter-plugin</url>
    </scm>

    <developers>
        <developer>
            <id>piotr-skotnicki</id>
            <name>Piotr Skotnicki</name>
            <email>piotr.skotnicki@gmail.com</email>
            <timezone>+1</timezone>
        </developer>
    </developers>

    <build>
        <plugins>
            <plugin>
                <groupId>org.jenkins-ci.tools</groupId>
                <artifactId>maven-hpi-plugin</artifactId>
                <version>1.64</version>
                <extensions>true</extensions>
                <configuration>
                <showDeprecation>true</showDeprecation>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <repositories>
        <repository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </repository>
    </repositories>

    <pluginRepositories>
        <pluginRepository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </pluginRepository>
    </pluginRepositories>
</project>  
  
